{
  "buildCommand": "cd frontend && rm -rf node_modules/.cache && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.js": {
      "memory": 256,
      "maxDuration": 10
    }
  }
}
